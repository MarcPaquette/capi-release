---
processes:
- name: redis
  executable: /var/vcap/packages/redis/redis-server
  args:
    - --port
    - 0
    - --unixsocket
    - /var/vcap/data/redis/redis.sock
    - --save
    - ""
  ephemeral_disk: true
  limits:
    open_files: 10032
  hooks:
    pre_start: /var/vcap/jobs/redis/bin/pre-start.sh
